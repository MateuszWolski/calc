<h5>Categories<h5>
  <ul class="collection">
    <%= for category <- @categories do %>
    <li class="collection-item"><%= category.name %>
      <div class="right">
        <%=link "Edit", to: category_path(@conn, :edit, category) %>
      </div>
    </li>
  <%end%>
</ul>


<div class="fixed-action-btn">
<%= link to: category_path(@conn, :new), class: "btn-floating btn-large waves-effect waves-light red" do %>
<i class="material-icons">add</i>
<% end %>
</a>
</div>
